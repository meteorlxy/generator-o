{
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    <%_ if (typescript) { _%>
    "typescript",
    "typescriptreact",
    <%_ } if (vue) { _%>
    "vue",
    <%_ } _%>
  ]<%_ if (vue) { _%>,
  "vetur.validation.template": false
  <%_ } _%>
}
